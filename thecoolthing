                        local response = syn.request({
                            Url = DiscordWebhook,
                            Method = 'POST',
                            Headers = {
                                ["Content-Type"] = "application/json"
                            },
                            Body = game:GetService('HttpService'):JSONEncode({
                                avatar_url = "https://cdn.discordapp.com/attachments/933294307429994556/971071145598140436/unknown.png",
                                content = ping,
                                username = "MXS",
                                embeds = embed
                            })
                        })
                        wait(2)
                        return
                    end 
                end
            end
        end)
    end)()
    --//AutoStore-Webhook-End

    --//MainModule-Start\\--
    local ObjectsFolder = Instance.new('Folder', game.Workspace)
    ObjectsFolder.Name = "IM4RH0nas2BPBBFDDYUC2VKw8uf9HonVkHg"
    local waitTime = 0

    for _,item in pairs(game:GetService("Workspace").ItemSpawns.StandardItems:GetDescendants()) do
        if table.find(GroundItems, item.Name) then
            item.Parent = ObjectsFolder
        end
    end
    for _,item2 in pairs(game.Workspace.ItemSpawns.Chests:GetDescendants()) do
        if item2.Name == "Chest" then
            item2.Parent = ObjectsFolder
        end
    end
    wait(2)
    for i = 1, #ObjectsFolder:GetChildren() do
        for _,item3 in pairs(ObjectsFolder:GetChildren()) do
            if item3.Name == "Chests" then
                waitTime = waitTime + 5
            elseif table.find(GroundItems, item3.Name) then
                waitTime = waitTime + 3
            end
        end
    end
    local AMorPM
    local currentHour = os.date("*t")["hour"]
    if currentHour < 12 or currentHour == 24 then
        AMorPM = 'AM'
    else
        AMorPM = 'PM'
    end
    for k = 1, #ObjectsFolder:GetChildren() do
        for _,consoleItem in pairs(ObjectsFolder:GetChildren()) do
            local itemName = consoleItem.Name
            rconsolename('MXS v2')
            rconsoleinfo('Username: '..fuefusiufjnfdsj..' -->'..' Found a '..tostring(itemName)..' --> '..'ServerId: '..game.JobId..' --> '..'Time: '..getTime()..' '..AMorPM)
        end
    end
    wait(1)
    spawn(function()
        while task.wait() do
            pcall(function()
                for i, v in pairs(ObjectsFolder:GetChildren()) do
                    if v.Name == "Chest" and v:FindFirstChild("RootPart") then
                        if v.RootPart:FindFirstChild("ProximityAttachment") then
                            getLocalCharacter().HumanoidRootPart.CFrame = v.RootPart.CFrame * CFrame.new(2,2,2)
                            if game:GetService("Players").LocalPlayer:DistanceFromCharacter(v.RootPart.Position) then 
                                if v.RootPart.ProximityAttachment:FindFirstChild("Interaction") then
                                    fireproximityprompt(v.RootPart.ProximityAttachment.Interaction, 5)
                                end
                            end
                        end
                    elseif table.find(GroundItems, v.Name) and v:FindFirstChild("ProximityAttachment") then
                        getLocalCharacter().HumanoidRootPart.CFrame = v.CFrame * CFrame.new(2,2,2)
                        if game:GetService("Players").LocalPlayer:DistanceFromCharacter(v.Position) then 
                            if v.ProximityAttachment:FindFirstChild("Interaction") then
                                fireproximityprompt(v.ProximityAttachment.Interaction, 5)
                            end
                        end
                    end
                end
            end)
        end
    end)
    wait(tonumber(waitTime))
    while task.wait() do
        if #ObjectsFolder:GetChildren() == 0 then
            --serverhop here
        end
    end
    --//MainModule-End\\--
else
    game.Players.LocalPlayer:Kick('Options: "chests", "ground", "both"')
    task.wait(0.01)
    while true do end
end
