                            Reason:find("Teleport") or Reason:find("place") then
                            task.wait(0.01)
                            game:GetService("TeleportService"):Teleport(6846458508, game:GetService("Players").LocalPlayer)
                        end
                    end
                end)
            end)
        end
    end)

    delay(AutoKick, function()
        --serverhop here
    end)
    --//Checkers-End\\--

    --//Credits-Start\\--
    local fuefusiufjnfdsj = game.Players.LocalPlayer.Name
    game.StarterGui:SetCore("SendNotification", {
        Title = "MXS",
        Text = "Welcome "..fuefusiufjnfdsj.."!, This script was made by the MXS developers team! https://discord.gg/ckJVZNPr7g",
        Duration = "5",
        Callback = NotificationBindable,
    })
    --//Credits-End\\--

    --//Functions-Start\\--
    local function getTime()
        local date = os.date("*t")
        return ("%02d:%02d"):format(((date.hour % 24) - 1) % 12 + 1, date.min)
    end
    --//Functions-End\\--

    --//AutoStore-Webhook-Start\\--
