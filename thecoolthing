                ["AddItems"] = true
            }
            game:GetService("ReplicatedStorage").Remotes.InventoryRemote:FireServer(ohString1, ohTable2)
        end
    end)()
    coroutine.wrap(function()
        game.Players.LocalPlayer.Backpack.ChildAdded:Connect(function(v)
            if string.len(DiscordWebhook) == 120 then
                if string.find(DiscordWebhook, "discord.com/api") then
                    if table.find(ItemsToStore, v.Name) then
                        local username = game.Players.LocalPlayer.name
                        local embed = {{
                            ["color"] = 3027255,
                            ["fields"] = {{
                                name = 'Username',
                                value = '||' .. username .. '||'
                            }, {
                                name = 'Item',
                                value = v.Name
                            }}
                        }}
            
                        local response = syn.request({
                            Url = DiscordWebhook,
                            Method = 'POST',
                            Headers = {
                                ["Content-Type"] = "application/json"
                            },
                            Body = game:GetService('HttpService'):JSONEncode({
                                avatar_url = "https://cdn.discordapp.com/attachments/933294307429994556/971071145598140436/unknown.png",
                                content = ping,
                                username = "MXS",
                                embeds = embed
                            })
                        })
                        wait(2)
                        return
                    end 
                end
            end
        end)
    end)()
    --//AutoStore-Webhook-End
    
    --//MainModule-Start\\--
    local ObjectsFolder = Instance.new('Folder', game.Workspace)
    ObjectsFolder.Name = "IM4RH0nas2BPBBFDDYUC2VKw8uf9HonVkHg"
    local waitTime = 0
    
    for _,item in pairs(game:GetService("Workspace").ItemSpawns.StandardItems:GetDescendants()) do
        if table.find(GroundItems, item.Name) then
            item.Parent = ObjectsFolder
        end
    end
    wait(2)
    for i = 1, #ObjectsFolder:GetChildren() do
        waitTime = waitTime + 3
    end
    local AMorPM
    local currentHour = os.date("*t")["hour"]
    if currentHour < 12 or currentHour == 24 then
        AMorPM = 'AM'
    else
        AMorPM = 'PM'
    end
    for k = 1, #ObjectsFolder:GetChildren() do
        for _,consoleItem in pairs(ObjectsFolder:GetChildren()) do
            local itemName = consoleItem.Name
            rconsolename('MXS v2')
            rconsoleinfo('Username: '..fuefusiufjnfdsj..' -->'..' Found '..tostring(itemName)..' --> '..'ServerId: '..game.JobId..' --> '..'Time: '..getTime()..' '..AMorPM)
        end
    end
    wait(1)
    spawn(function()
        while task.wait() do
            pcall(function()
                for i, v in pairs(ObjectsFolder:GetChildren()) do
                    if table.find(GroundItems, v.Name) and v:FindFirstChild("ProximityAttachment") then
                        getLocalCharacter().HumanoidRootPart.CFrame = v.CFrame * CFrame.new(2,2,2)
                        if game:GetService("Players").LocalPlayer:DistanceFromCharacter(v.Position) then 
                            if v.ProximityAttachment:FindFirstChild("Interaction") then
                                fireproximityprompt(v.ProximityAttachment.Interaction, 5)
                            end
                        end
                    end
                end
            end)
        end
    end)
    wait(tonumber(waitTime))
    while task.wait() do
        if #ObjectsFolder:GetChildren() == 0 then
            --serverhop here
        end
    end
    --//MainModule-End\\--
elseif hopperType == "both" then
    --//Loading-Start\\--
    warn('Made by the MXS developers team.')
    repeat wait() until game:IsLoaded()
    repeat wait() until game.Players.LocalPlayer
    if game.PlaceId == 5130598377 then
        game:GetService("TeleportService"):TeleportToPlaceInstance(6846458508, game.Players.LocalPlayer)
    end
    repeat wait() until game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("DataLoading")
