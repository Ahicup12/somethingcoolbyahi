            wait(0.1)
            local ohString1 = "ItemInventory"
            local ohTable2 = {
                ["AddItems"] = true
            }
            game:GetService("ReplicatedStorage").Remotes.InventoryRemote:FireServer(ohString1, ohTable2)
        end
    end)()
    coroutine.wrap(function()
        game.Players.LocalPlayer.Backpack.ChildAdded:Connect(function(v)
            if string.len(DiscordWebhook) == 120 then
                if string.find(DiscordWebhook, "discord.com/api") then
